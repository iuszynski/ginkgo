@Article{chateauvieux2007,
  Title                    = {{Molecular profile of mouse stromal mesenchymal stem
 cells}},
  Author                   = {Chateauvieux, S. and Ichant{\'{e}}, J.-L. and Delorme, B.
 and Frouin, V. and Pi{\'{e}}tu, G. and Langonn{\'{e}}, A.
 and Gallay, N. and Senseb{\'{e}}, L. and Martin, M.T. and
 Moore, K.A. and Charbord, P.},
  Journal                  = {Physiological Genomics},
  Year                     = {2007},
  Number                   = {2},
  Volume                   = {29},

  Abstract                 = {We determined a transcriptional profile specific for
 clonal stromal mesenchymal stem cells from adult and fetal
 hematopoietic sites. To identify mesenchymal stem cell-like
 stromal cell lines, we evaluated the adipocytic,
 osteoblastic, chondrocytic, and vascular smooth muscle
 differentiation potential and also the hematopoietic
 supportive (stromal) capacity of six mouse stromal cell
 lines from adult bone marrow and day 14.5 fetal liver. We
 found that two lines were quadripotent and also supported
 hematopoiesis, BMC9 from bone marrow and AFT024 from fetal
 liver. We then ascertained the set of genes differentially
 expressed in the intersection set of AFT024 and BMC9
 compared with those expressed in the union set of two
 negative control lines, 2018 and BFC012 (both from fetal
 liver); 346 genes were upregulated and 299 downregulated.
 Using Ingenuity software, we found two major gene networks
 with highly significant scores. One network contained
 downregulated genes that are known to be implicated in
 osteoblastic differentiation, proliferation, or
 transformation. The other network contained upregulated
 genes that belonged to two categories, cytoskeletal genes
 and genes implicated in the transcriptional machinery. The
 data extend the concept of stromal mesenchymal stem cells
 to clonal cell populations derived not only from bone
 marrow but also from fetal liver. The gene networks
 described should discriminate this cell type from other
 types of stem cells and help define the stem cell state.
 Copyright {\textcopyright} 2007 the American Physiological
 Society.},
  Doi                      = {10.1152/physiolgenomics.00197.2006},
  ISSN                     = {10948341},
  Keywords                 = {Cytoskeleton,Differentiation,Gene
 network,Hematopoiesis,Transcription}
}

