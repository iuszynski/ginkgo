@Article{foehrenbach1999,
  Title                    = {{Development of an automatic multimodality registration
 application for clinical brain imaging}},
  Author                   = {Foehrenbach, H. and {De Dreuille}, O. and Mangin, J.F. and
 Lev{\^{e}}que, C. and Delaye, F. and Lathuilli{\`{e}}re,
 J.M. and Frouin, V. and Cordoliani, Y.S. and Gaillard, J.F.},
  Journal                  = {Medecine Nucleaire},
  Year                     = {1999},
  Number                   = {4},
  Volume                   = {23},

  Abstract                 = {Aim: Image coregistration has become necessary due to the
 growing number of cerebral imaging modalities. Interactive
 registration is easy to implement but is laborious and
 highly operator dependant. Utilization of automatic methods
 is easier but the development of such applications is much
 more complex. The aim of this work was to bring in clinical
 practice a fully unsupervised registration method initially
 designed for PET and MRI. Material, methods: MRI and SPECT
 images, 99m Tc-ECD and thallium 201, of 74 patients were
 registered during the development phase. The principle of
 the method used is external surface matching. Registered
 images were visualized with a specifically designed tool.
 Results : Registration was entirely automatic in 80{\%} of
 the cases. In the 20{\%} left, MRI and SPECT artifacts
 imposed simple modifications of the programs parameters to
 obtain a satisfactory result. Mean time for the whole
 process is 7 mn. Different superposition techniquse have
 been proposed according to the image kind. Conclusion :
 These results support the major importance of image quality
 in the successful utilization of automatic registration
 methods. Testing of the different superimposition modes
 indicates that the chosen one has to be fitted to the kind
 of images, particularly to the distribution of the
 scintigraphic tracer. The disponibility of such automatic
 tools introduces in clinical practice a new reading of
 scintigraphic information, anatomical structure recognition
 being no more a primary constraint.},
  ISSN                     = {09281258},
  Keywords                 = {Automatic multimodality registration,Cerebral MRI,Cerebral
 SPECT,Clinical practice}
}

@InProceedings{fouque2013,
